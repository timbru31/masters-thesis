	Isbn = {978-3-319-05452-0},
	Month = {11},
	Pages = {184--201},
	Publisher = {Springer International Publishing},
	Title = {KeySens: Passive User Authentication through Micro-behavior Modeling of Soft Keyboard Interaction},
	Year = {2014},
