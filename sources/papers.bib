	Isbn = {978-0-12-416688-2},
	Keywords = {identity management, mobile identity management, digital identity, privacy, user centricity, usability, centralized identity management, federation identity management, meta-directories, virtual directories},
	Pages = {75 - 118},
	Publisher = {Syngress},
	Title = {Online Identity and User Management Services},
	Year = {2014},
